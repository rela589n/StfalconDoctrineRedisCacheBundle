{
    "name": "stflacon/doctrine-redis-cache-bundle",
    "description": "Custom implementation of Predis cache provider for Doctrine cache.",
    "keywords": ["bundle", "symfony", "doctrine", "cache", "redis"],
    "homepage": "https://github.com/stfalcon-studio/DoctrineRedisCacheBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/stfalcon-studio/DoctrineRedisCacheBundle/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "doctrine/doctrine-bundle": "^1.6.10"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpunit/phpunit": "^7.5",
        "escapestudios/symfony2-coding-standard": "^3.4",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "StfalconStudio\\DoctrineRedisCacheBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "StfalconStudio\\DoctrineRedisCacheBundle\\Tests\\": "Tests/"
        }
    }
}
